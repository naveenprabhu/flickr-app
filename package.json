{
  "name": "flickr-app",
  "version": "1.0.0",
  "description": "Microservice to communicate to flickr",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint -c .eslintrc.js \"{config,src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint -c .eslintrc.js \"{config,src,test}/**/*.{js,jsx,ts,tsx}\" --quiet --fix",
    "test": "jest --testPathIgnorePatterns .st.test. --coverage --verbose",
    "test:st": "jest --testPathIgnorePatterns '\\.(int|unit)\\.test\\.' --coverage --verbose --runInBand",
    "test:all": "jest --coverage --verbose --runInBand",
    "clearCache": "jest --clearCache",
    "start": "node dist/src/index.js",
    "start:dev": "NODE_ENV=dev ts-node src/index.ts",
    "commit": "cz"
  },
  "author": "Naveen Arumugam",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^1.2.2",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.3.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/hapi__joi": "^17.1.9",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/pem-jwk": "^2.0.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "axios-mock-adapter": "^1.21.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "express-openid-connect": "^2.11.0",
    "fastify": "^4.11.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-allure": "^0.1.3",
    "pem-jwk": "^2.0.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
